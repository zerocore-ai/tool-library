{
  "manifest_version": "0.3",
  "name": "todolist",
  "version": "0.1.0",
  "description": "An MCP server",
  "author": {
    "name": "Stephen Akinyemi"
  },
  "server": {
    "type": "binary",
    "entry_point": "target/release/todolist",
    "mcp_config": {
      "command": "${__dirname}/target/release/todolist"
    }
  },
  "compatibility": {
    "platforms": [
      "darwin"
    ]
  },
  "_meta": {
    "company.superrad.radical": {
      "scripts": {
        "build": "cargo build --release"
      }
    }
  }
}